.grid.flex
    - var a = 0;
    - for (i = 1; i <= 3; i++) {
        .big_row.flex
            - for (j = 1; j <= 3; j++) {
                - a++;
                div(data-big-cell = a).big_cell.flex
                    - var b = 0;
                    - for (k = 1; k <= 3; k++) {
                        .small_row.flex
                            - for (l = 1; l <= 3; l++) {
                                - b++;
                                div(data-small-cell = b).small_cell.flex
                            - }
                    - }
            - }
    - }